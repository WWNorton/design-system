{
  "name": "@wwnds/react",
  "version": "1.0.1",
  "description": "React components for the Norton Design System.",
  "keywords": [
    "component",
    "components",
    "design system",
    "react",
    "typescript"
  ],
  "homepage": "https://github.com/wwnorton/design-system",
  "bugs": "https://github.com/wwnorton/design-system/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/wwnorton/design-system",
    "directory": "packages/react"
  },
  "license": "MIT",
  "author": "Evan Yamanishi <eyamanishi@wwnorton.com>",
  "contributors": [
    "Gabriel Rumbaut <grumbaut@wwnorton.com>",
    "HonChih Chen <hchen@wwnorton.com>",
    "Anand Patil <apatil@wwnorton.com>",
    "Jonathan Chan <jchan@wwnorton.com>"
  ],
  "main": "dist/index.js",
  "unpkg": "dist/index.umd.js",
  "module": "dist/index.module.js",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run build:main & npm run build:umd",
    "build:main": "microbundle --jsx React.createElement --jsxFragment React.Fragment --format modern,es,cjs --globals @popperjs/core=Popper,react=React,react-dom=ReactDOM",
    "build:umd": "microbundle --jsx React.createElement --jsxFragment React.Fragment --format umd --name NDSReact --external @popperjs/core,localforage,react,react-dom --globals @popperjs/core=Popper,react=React,react-dom=ReactDOM",
    "build:stories": "build-storybook",
    "dev:stories": "start-storybook -p 9009"
  },
  "dependencies": {
    "@material/linear-progress": "^10.0.0",
    "@popperjs/core": "^2.4.4",
    "classnames": "^2.2.6",
    "localforage": "^1.9.0",
    "lodash": "^4.17.20",
    "react-fast-compare": "^3.2.0"
  },
  "devDependencies": {
    "@wwnds/core": "^1.0.1"
  },
  "peerDependencies": {
    "react": "^16.0.0 || ^17.0.0",
    "react-dom": "^16.0.0 || ^17.0.0"
  }
}
